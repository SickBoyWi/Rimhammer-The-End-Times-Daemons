	<PawnKindDef ParentName="RH_TET_BaseDaemonKind">
		<defName>RH_TET_Daemons_Bloodletter</defName>
		<label>bloodletter</label>
		<race>RH_TET_Daemon_Bloodletter</race>
		<combatPower>115</combatPower>
		<lifeStages>
			<li>
				<bodyGraphicData>
					<texPath>Things/Pawn/Bloodletter/Unarmed/Bloodletter</texPath>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>2.0</drawSize>
					<shadowData>
						<volume>(0.4, 0.8, 0.4)</volume>
					</shadowData>
				</bodyGraphicData>
			</li>
		</lifeStages>
		<weaponMoney>0~0</weaponMoney>
		<defendPointRadius>0</defendPointRadius>
		<weaponTags>
		</weaponTags>
		<techHediffsChance>0</techHediffsChance>
		<techHediffsMoney>9999~9999</techHediffsMoney>
	</PawnKindDef>
	
